{
  "name": "jcore-vue-broiler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-watch": "vite build -w",
    "preview": "vite preview",
    "build-only": "vite build",
